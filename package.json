{
    "name": "smart-express",
    "description": "A sample Node.js Web App with Express.js and Zurb Foundation for Sites 6",
    "version": "0.1.7",
    "private": true,
    "engines": {
        "node": ">=10.13.0"
    },
    "browserslist": [
        "Edge >= 17",
        "> 1.0% in IT"
    ],
    "babel": {
        "minified": true,
        "comments": false,
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "entry",
                    "corejs": 3
                }
            ]
        ]
    },
    "scripts": {
        "start": "node app.js",
        "build": "gulp build",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettify": "prettier --single-quote --trailing-comma es5 --write \"routes/*.js\" \"public/js/*.js\""
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "foundation-sites": "^6.7.0",
        "hideshowpassword": "^2.2.0",
        "jquery": "^3.5.1",
        "nunjucks": "^3.2.3",
        "request": "^2.88.2",
        "serve-favicon": "^2.5.0",
        "what-input": "^5.2.10",
        "winston": "^2.4.5"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-babel": "^5.3.1",
        "debug": "^4.3.1",
        "eslint": "^7.16.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-less": "^4.0.1",
        "prettier": "2.2.1"
    },
    "author": "Alessio Saltarin",
    "license": "MIT"
}
