{
    "name": "smart-express",
    "description": "A sample Node.js Web App with Express.js and Zurb Foundation for Sites 6",
    "version": "0.1.1",
    "private": true,
    "engines": {
        "node": ">=10.13.0"
    },
    "browserslist": [
        "Edge >= 17",
        "> 1.0% in IT"
    ],
    "babel": {
        "minified": true,
        "comments": false,
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "entry",
                    "corejs": 3
                }
            ]
        ]
    },
    "scripts": {
        "start": "node app.js",
        "build": "gulp build",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettify": "prettier --single-quote --trailing-comma es5 --write \"routes/*.js\" \"public/js/*.js\""
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cfenv": "^1.2.2",
        "cookie-parser": "^1.4.4",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-session": "^1.16.2",
        "foundation-sites": "^6.5.3",
        "hideshowpassword": "^2.1.1",
        "jquery": "^3.4.1",
        "nunjucks": "^3.2.0",
        "request": "^2.88.0",
        "serve-favicon": "~2.5.0",
        "what-input": "^5.2.1",
        "winston": "^2.4.4"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "debug": "^4.1.1",
        "eslint": "^6.0.1",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-less": "^4.0.1",
        "prettier": "1.18.2"
    },
    "author": "Alessio Saltarin",
    "license": "ISC"
}
